{
    "name": "Calculate PI",
    "desc": "This Simple Job Calculating PI.",
    "kernel": "var iterations=params.iterations;var Pi=0;var n=1;for(i=0;i<=iterations;i++){Pi=Pi+(4/n)-(4/(n+2));n=n+4;} result=Pi;",
    "params": {
        "iterations": 999999
    },
    "isPartitioned": false,
    "hasAftBarr": false,
    "afBarrFunc": ""
}
